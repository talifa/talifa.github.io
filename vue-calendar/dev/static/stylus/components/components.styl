/* Стили календаря */
.new-calendar {
    .jsInfobox {
        background: white;
        box-shadow: 0 0 5px #555;
        border-radius: 4px;
        position: absolute;
        display: none;
        padding: 15px;
        z-index: 2;
        min-height: auto;
        top: 50px;
        left: -10px;
        bottom: inherit;
        right: inherit;

        .jsEnterbox {
            min-width: 270px;

            &:before {
                display: block;
                content: '';
                position: absolute;
                width: 15px;
                height: 15px;
                left: 30px;
                top: -6px;
                background: #fff;
                border-radius: 2px;
                transform: rotate(-45deg);
                // box-shadow: 0 0 5px #555;
                box-shadow: 3px -3px 10px -3px rgba(85, 85, 85, 0.9);
            }

            .jsEnterboxForm {
                display: flex;
                justify-content: space-between;

                // padding: 15px;
                // width 100%
                .jsEnterboxInput {
                    max-width: 120px;
                }
            }
        }
    }

    .allProdView {
        .datepicker--cell {
            box-shadow: none !important;
            pointer-events: none !important;
        }

        .-selected- {
            background: transparent !important;
            color: #1a1f23 !important;
        }
    }

    .del-btn {
        background: transparent;
        background-image: url("data:image/svg+xml,%3Csvg width='14' height='18' viewBox='0 0 14 18' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath fill-rule='evenodd' clip-rule='evenodd' d='M1 16C1 17.1 1.9 18 3 18H11C12.1 18 13 17.1 13 16V4H1V16ZM14 1H10.5L9.5 0H4.5L3.5 1H0V3H14V1Z' fill='%23D6D6D6' /%3E '%3C/svg%3E");
        padding: 0 15px;
        background-position: 50% 50%;
        background-repeat: no-repeat;
    }

    .btn-submit {
        // background: transparent;
        // background-image: url("data:image/svg+xml,%0A%3Csvg version='1.1' id='Capa_1' fill='%23D6D6D6' xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink' x='0px' y='0px' viewBox='0 0 512 512' style='enable-background:new 0 0 512 512;' xml:space='preserve'%3E%3Cg%3E%3Cg%3E%3Cpath d='M492,236H276V20c0-11.046-8.954-20-20-20c-11.046,0-20,8.954-20,20v216H20c-11.046,0-20,8.954-20,20s8.954,20,20,20h216 v216c0,11.046,8.954,20,20,20s20-8.954,20-20V276h216c11.046,0,20-8.954,20-20C512,244.954,503.046,236,492,236z'/%3E%3C/g%3E%3C/g%3E%3Cg%3E%3C/g%3E%3Cg%3E%3C/g%3E%3Cg%3E%3C/g%3E%3Cg%3E%3C/g%3E%3Cg%3E%3C/g%3E%3Cg%3E%3C/g%3E%3Cg%3E%3C/g%3E%3Cg%3E%3C/g%3E%3Cg%3E%3C/g%3E%3Cg%3E%3C/g%3E%3Cg%3E%3C/g%3E%3Cg%3E%3C/g%3E%3Cg%3E%3C/g%3E%3Cg%3E%3C/g%3E%3Cg%3E%3C/g%3E%3C/svg%3E%0A");
        padding: 0 15px;
        background-position: 50% 50%;
        background-repeat: no-repeat;
    }

    .calendar-box_info:before {
        display: none;
    }

    .calendar__datepicker {
        // flex: 0 0 100%;
        // max-width: 550px;
    }

    .datepicker {
        width: 100%;
        border: none;
    }

    .ItemHelpTextCommon {
        padding-left: 20px;

        span {
            width: 311px;
            height: 20px;
            font-size: 13px;
            font-weight: normal;
            font-style: normal;
            font-stretch: normal;
            line-height: 1.54;
            letter-spacing: normal;
            color: #6a6a6a;
        }
    }

    .calendar__sidebar {
        // padding: 0;
        // padding-right: 15px;
        #pr-list {
            // border: 1px solid #d6d6d6;
            box-sizing: border-box;

            // костыль для IE
            .pr-list-item.active {
                justify-content: flex-start;

                .quantity {
                    position: absolute;
                    right: 0;
                }
            }

            .pr-list-item {
                border-radius: 4px;
                cursor: pointer;
                display: inline-block;
                width: 100%;
                border: 1px solid #d6d6d6;
                font-weight: bold;
                line-height: 20px;
                font-size: 14px;
                padding: 20px 0 20px 20px;
                color: #181818;
                display: flex;
                justify-content: space-between;

                // min-height: 50px;
                &:first-of-type {
                    border-bottom: 0;
                    border-bottom-right-radius: 0;
                    border-bottom-left-radius: 0;
                }

                &:not(:first-of-type):not(:last-of-type) {
                    border-radius: 0;
                }

                &:not(:first-of-type):not(:last-of-type):not(.active) {
                    border-bottom: 0;
                }

                &:last-of-type {
                    border-top-left-radius: 0;
                    border-top-right-radius: 0;
                }

                &.active {
                    border: 2px solid #586069;
                    // box-shadow: inset 0 0 0px 1px #586069;
                    position: relative;
                    color: #586069;
                    background: #fff;

                    &:before {
                        display: block;
                        content: '';
                        position: absolute;
                        width: 25px;
                        height: 25px;
                        right: -14px;
                        top: 16px;
                        background: #fff;
                        border-radius: 4px;
                        -webkit-transform: rotate(-45deg);
                        transform: rotate(-45deg);
                        border: 2px solid #586069;
                        border-top: none;
                        border-left: none;
                        border-top-right-radius: 0;
                        border-bottom-left-radius: 0;
                    }
                }

                .quantity {
                    padding-right: 20px;
                    font-style: normal;
                    font-weight: normal;
                    line-height: 18px;
                    font-size: 13px;
                    text-align: right;
                    word-break: break-all;
                    color: #f98514;

                    i {
                        font-style: normal;
                    }
                }

                .quantity.totals {
                    color: #586069 !important;
                }

                .quantity.over {
                    color: red !important;
                    opacity: 0.8;
                }

                .product-title {
                    display: block;
                    max-width: 370px;
                    float: left;
                }
            }
        }
    }

    .datepicker--cell-day {
        width: 12.28571%;
        margin: 1%;
        display: flex;
        flex-direction: column;
        z-index: 0;
        justify-content: space-around;
        height: 57px;
    }

    .carempty {
        display: block;
        width: 16px;
        height: 12px;
        padding: 0;
        margin: 5px 0;
        margin-bottom: 5px;
    }

    .carlabel {
        background-image: url("data:image/svg+xml,%3Csvg width='16' height='12' viewBox='0 0 16 12' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath fill-rule='evenodd' clip-rule='evenodd' d='M13.3333 3.33366H11.3333V0.666992H1.99996C1.26663 0.666992 0.666626 1.26699 0.666626 2.00033V9.33366H1.99996C1.99996 10.4403 2.89329 11.3337 3.99996 11.3337C5.10663 11.3337 5.99996 10.4403 5.99996 9.33366H9.99996C9.99996 10.4403 10.8933 11.3337 12 11.3337C13.1066 11.3337 14 10.4403 14 9.33366H15.3333V6.00033L13.3333 3.33366ZM3.99996 10.3337C3.44663 10.3337 2.99996 9.88699 2.99996 9.33366C2.99996 8.78032 3.44663 8.33366 3.99996 8.33366C4.55329 8.33366 4.99996 8.78032 4.99996 9.33366C4.99996 9.88699 4.55329 10.3337 3.99996 10.3337ZM13 4.33366L14.3066 6.00033H11.3333V4.33366H13ZM12 10.3337C11.4466 10.3337 11 9.88699 11 9.33366C11 8.78032 11.4466 8.33366 12 8.33366C12.5533 8.33366 13 8.78032 13 9.33366C13 9.88699 12.5533 10.3337 12 10.3337Z' fill='%236A6A6A'/%3E%3C/svg%3E");
        background-repeat: no-repeat;
    }

    .carlabel-other {
        background-image: url("data:image/svg+xml;charset=utf8,%3Csvg width='16' height='12' viewBox='0 0 16 12' fill='none' xmlns='http://www.w3.org/2000/svg'%3E%3Cpath fill-rule='evenodd' clip-rule='evenodd' d='M13.3334 3.33366H11.3334V0.666992H2.00008C1.26675 0.666992 0.666748 1.26699 0.666748 2.00033V9.33366H2.00008C2.00008 10.4403 2.89341 11.3337 4.00008 11.3337C5.10675 11.3337 6.00008 10.4403 6.00008 9.33366H10.0001C10.0001 10.4403 10.8934 11.3337 12.0001 11.3337C13.1067 11.3337 14.0001 10.4403 14.0001 9.33366H15.3334V6.00033L13.3334 3.33366ZM4.00008 10.3337C3.44675 10.3337 3.00008 9.88699 3.00008 9.33366C3.00008 8.78032 3.44675 8.33366 4.00008 8.33366C4.55341 8.33366 5.00008 8.78032 5.00008 9.33366C5.00008 9.88699 4.55341 10.3337 4.00008 10.3337ZM13.0001 4.33366L14.3067 6.00033H11.3334V4.33366H13.0001ZM12.0001 10.3337C11.4467 10.3337 11.0001 9.88699 11.0001 9.33366C11.0001 8.78032 11.4467 8.33366 12.0001 8.33366C12.5534 8.33366 13.0001 8.78032 13.0001 9.33366C13.0001 9.88699 12.5534 10.3337 12.0001 10.3337Z' fill='%23D6D6D6'/%3E%3C/svg%3E");
        background-repeat: no-repeat;
    }

    .tooltip-car::after {
        background: #F2F2F2;
        border-radius: 6px;
        // box-shadow: 1px 1px 10px rgba(0, 0, 0, 0.5);
        content: attr(data-tooltip-car); /* Главная часть кода, определяющая содержимое всплывающей подсказки */
        // margin-top: -24px;
        opacity: 0; /* Наш элемент прозрачен... */
        padding: 8px;
        font-style: normal;
        font-weight: normal;
        font-size: 12px;
        line-height: 14px;
        width: 270px;
        color: #1A1F23;
        position: absolute;
        visibility: hidden; /* ...и скрыт. */
        z-index: 100;
        bottom: -37px;
        left: -83px;
        transition: all 0.4s ease-in-out; /* Добавить плавности по вкусу */
    }

    .tooltip-car:hover::after {
        opacity: 1; /* Показываем его */
        visibility: visible;
    }

    .datepicker--nav div:nth-child(2) {
        order: -1;
    }

    .datepicker--nav {
        border: none;

        // pointer-events: none;
        .datepicker--nav-action {
            // display: none;
        }
    }

    .datepicker--days-names {
        background: #f5f5f5;
        padding: 5px 0;

        .datepicker--day-name {
            font-style: normal;
            font-weight: 500;
            line-height: 20px;
            font-size: 13px;
            text-align: center;
            color: #6a6a6a;
        }
    }

    .datepicker--nav-title {
        font-style: normal;
        font-weight: normal;
        line-height: 18px;
        font-size: 13px;
        color: #1a1f23;
    }

    .datepicker--cell {
        font-style: normal;
        font-weight: normal;
        line-height: 24px;
        font-size: 16px;
        text-align: center;
        -webkit-box-shadow: inset 0px 0px 0px 1px rgba(235, 235, 235, 1);
        -moz-box-shadow: inset 0px 0px 0px 1px rgba(235, 235, 235, 1);
        box-shadow: inset 0px 0px 0px 1px rgba(235, 235, 235, 1);
        box-sizing: border-box;
        border-radius: 4px;
        position: relative;
        z-index: auto;
        color: #1a1f23;

        small {
            font-style: normal;
            font-weight: 500;
            line-height: 12px;
            font-size: 10px;
            text-align: center;
            color: #586069;
        }

        &.-disabled- {
            color: #9b9c9e;
            box-shadow: inset 0px 0px 0px 1px rgba(235, 235, 235, 0);
        }

        &.-current- {
            color: #586069;
            font-weight: bold;
        }

        &.-filled- {
            background: rgba(0, 140, 149, 0.1) !important;
            font-weight: bold;
        }

        &.-selected- {
            color: #586069;
            font-weight: normal;
            background: rgba(0, 140, 149, 0.05);
            // border: 1px solid #586069;
            box-shadow: inset 0px 0px 0px 1px #586069;

            &.-focus- {
                font-weight: normal;
                box-shadow: inset 0px 0px 0px 1px #586069;
            }
        }

        &.-focus- {
            font-weight: bold;
            background: rgba(0, 140, 149, 0.05);
            box-shadow: inset 0px 0px 0px 1px rgba(235, 235, 235, 0);

            &.-disabled- {
                cursor: not-allowed;
                font-weight: normal;
                background: transparent;
                box-shadow: inset 0px 0px 0px 1px rgba(235, 235, 235, 0);
            }
        }

        &.-other-month- {
            cursor: not-allowed;
            visibility: hidden;
        }
    }

    .datepicker--cells-days {
        height: 100%;
        width: 100%;
    }
}

.wide-box {
    padding: 20px 0;
}

.wide-box > div {
    width: 100%;
}
